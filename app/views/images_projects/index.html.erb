<h1>Listing images_projects</h1>

<table>
  <thead>
    <tr>
      <th>Id projeto</th>
      <th>Img</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images_projects.each do |images_project| %>
      <tr>
        <td><%= images_project.id_projeto %></td>
        <td><%= images_project.img %></td>
        <td><%= images_project.tipo %></td>
        <td><%= link_to 'Show', images_project %></td>
        <td><%= link_to 'Edit', edit_images_project_path(images_project) %></td>
        <td><%= link_to 'Destroy', images_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Images project', new_images_project_path %>
